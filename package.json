{
  "name": "factory.io",
  "version": "0.0.5",
  "description": "Modern class based mock data generation with typescript support.",
  "keywords": [
    "factory",
    "class",
    "test",
    "fixtures",
    "typescript",
    "typeorm"
  ],
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "author": "Krzysztof Olipra",
  "license": "MIT",
  "repository": "https://github.com/Nalhin/factory.io",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" README.md",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix && tsc --p tsconfig.build.json --emitDeclarationOnly",
    "publish": "npm run build && npm login && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
  }
}
